platform :ios, '10.3'
use_frameworks!
inhibit_all_warnings!

def myPods
    pod 'Alamofire', '~> 4.5'
    pod 'AlamofireNetworkActivityIndicator', '~> 2.2'
    pod 'SwiftyJSON'
    pod 'Kingfisher', '~> 4.6.0'
    pod 'IOStickyHeader'
    pod 'RealmSwift', '~> 3.1.1'
    pod 'AICustomViewControllerTransition'

end

target 'SeriesHub' do
    myPods
end

